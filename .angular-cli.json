{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "trabeller-co-use"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "vendors",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "trb",
      "styles": [        
        "vendors/cubeportfolio/cubeportfolio.css",
        "styles/app.scss",
        "styles/bootstrap.scss",
        "../node_modules/bootstrap/dist/css/bootstrap.css",
        "styles/font-awesome.scss",
        "styles/layout.scss",
        "styles/loader.scss",
        "styles/material2-theme.scss",
        "styles/theme.scss",
        "styles/ui.scss",
        "styles/trabbie.scss",
        "../node_modules/leaflet/dist/leaflet.css",
        "styles/ext/layout.min.css",
        "styles/ext/profile-2.min.css"        
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/leaflet/dist/leaflet.js",
        "vendors/cubeportfolio/jquery.cubeportfolio.js",
        "vendors/cubeportfolio/portfolio-1.js",
        "vendors/bootstrap.min.js"
        
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {}
  }
}